<section class="card">
  <h2>Tutorials</h2>
  <div class="content">
    <fieldset>
      <legend>Section Jump</legend>
      <div class="content alignStart flexWrap" style="padding-top:0;">
        <% for (let typeName of Object.keys(tutorialsList)) { %>
          <div class="w25p"><a role="button" class="spacer5-all secondary dispBlock textAlignLeft" href="#<%= typeName.replace(' ', '_') %>"><%= typeName %></a></div>
        <% } %>
      </div>
    </fieldset>
  </div>
</section>
<% for (let [type, tutorials] of Object.entries(tutorialsList)) { %>
  <section class="card" id="<%= type.replace(' ', '_') %>">
    <h2><%= type %></h2>
    <div class="content">
      <% for (let tutorial of tutorials) { %>
        <textarea readonly class="w100p wikitext autosize" spellcheck="false"><%= tutorial.Wikitext %></textarea>
      <% } %>
    </div>
  </section>
<% } %>